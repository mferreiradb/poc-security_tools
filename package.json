{
  "name": "eslint-security-poc",
  "version": "1.0.0",
  "description": "POC para testar plugins de seguran√ßa do ESLint com TypeScript",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "lint:all-problems": "eslint examples/*/problem.ts",
    "lint:all-refactors": "eslint examples/*/refactor.ts",
    "lint:backend": "eslint examples/backend-vulnerabilities/",
    "lint:backend:problem": "eslint examples/backend-vulnerabilities/problem.ts",
    "lint:backend:refactor": "eslint examples/backend-vulnerabilities/refactor.ts",
    "lint:complex": "eslint examples/complex-code/",
    "lint:complex:problem": "eslint examples/complex-code/problem.ts",
    "lint:complex:refactor": "eslint examples/complex-code/refactor.ts",
    "lint:frontend": "eslint examples/frontend-xss/",
    "lint:frontend:problem": "eslint examples/frontend-xss/problem.ts",
    "lint:frontend:refactor": "eslint examples/frontend-xss/refactor.ts",
    "lint:api": "eslint examples/api-security/",
    "lint:api:problem": "eslint examples/api-security/problem.ts",
    "lint:api:refactor": "eslint examples/api-security/refactor.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "security",
    "poc",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dompurify": "^3.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-security-node": "^1.1.4",
    "eslint-plugin-sonarjs": "^0.24.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "jsonwebtoken": "^7.4.1",
    "react": "^19.2.3"
  }
}
